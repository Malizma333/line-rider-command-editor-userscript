{
  "extends": "airbnb",
  "settings": {
    "react": {
      "version": "16.3.1"
    }
  },
  "rules": {
    "react/no-unused-class-component-methods": "off",
    "linebreak-style":"off"
  },
  "ignorePatterns": [
    "node_modules",
    "*.user.js",
    "*.min.js"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "Actions": "readonly",
    "ScriptGenerator": "readonly",
    "InitComponentClass": "readonly",
    "ComponentManager": "readonly",
    "Constants": "readonly",
    "ScriptParser": "readonly",
    "Selectors": "readonly",
    "Styles": "readonly",
    "Validator": "readonly"
  }
}